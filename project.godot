config_version=5

[application]

config/name="Ping Ping"
run/main_scene="res://Scenes/game_manager.tscn"
config/features=PackedStringArray("4.2", "C#", "Mobile")
config/icon="res://ping-svgrepo-com.svg"

[display]

window/size/viewport_width=960
window/size/viewport_height=540

[dotnet]

project/assembly_name="Ping Ping"

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="Platform"
2d_physics/layer_2="Ball"
2d_physics/layer_3="PlatformDestroyer"

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
